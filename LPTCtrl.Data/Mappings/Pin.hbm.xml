<?xml version="1.0" encoding="utf-8" ?>

<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" assembly="LPTCtrl.Domain" namespace="LPTCtrl.Data.Domain">
	<class name="Pin" table="PIN">
		<id name="ID" column="ID" type="int">
			<generator class="native" />
		</id>
		<property name="Bit" column="BIT" type="int" not-null="true" />
		<property name="Name" column="NAME" type="string"/>
		<many-to-one name="Port" column="PORTID" class="Port" not-null="true" />
	</class>
</hibernate-mapping>
